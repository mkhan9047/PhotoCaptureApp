<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="app.photocapture.com">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name="app.photocapture.PhotoCaptureApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".qr_code_scan.QrCodeScanActivity"></activity>
        <activity
            android:name=".setting_activity.SettingActivity"
            android:parentActivityName=".photo_capture_activity.PhotoCaptureActivity" />
        <activity android:name=".photo_capture_activity.PhotoCaptureActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>